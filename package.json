{
	"name": "pm2-web",
	"version": "1.6.3",
	"description": "A web based monitor for PM2",
	"preferGlobal": true,
	"main": "pm2-web.js",
	"scripts": {
		"integration": "istanbul cover nodeunit test/integration",
		"test": "istanbul cover nodeunit test/unit",
		"coveralls": "istanbul cover nodeunit test/unit && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/achingbrain/pm2-web.git"
	},
	"dependencies": {
		"express": "^3.4",
		"jade": "^1.1",
		"winston": "^0.7",
		"underscore.string": "^2.3",
		"wantsit": "^1.0",
		"pm2-interface": "^0.1",
		"ws": "^0.4",
		"wildemitter": "^1.0",
		"cjson": "^0.3",
		"minimist": "^0.0",
		"semver": "^2.2",

		"zepto-browserify": "^1.0",
		"moment": "^2.5",
		"mustache": "^0.8",
		"ansi-to-html": "^0.1",
		"html-entities": "^1.0"
	},
	"optionalDependencies": {
		"mdns2": "^2.1.0"
	},
	"devDependencies": {
		"grunt-browserify": "^1.3",
		"grunt-contrib-watch": "^0.5",
		"grunt-contrib-less": "^0.9",
		"nodeunit": "^0.8",
		"should": "^3.1",
		"sinon": "^1.8",
		"istanbul": "^0.2",
		"coveralls": "^2.8",
		"protractor": "^0.19",
		"proxyquire": "^0.4"
	},
	"preferGlobal": "true",
	"bin": {
		"pm2-web" : "bin/pm2-web.sh"
	},
	"keywords": [
		"pm2",
		"monitor",
		"web",
		"ui",
		"process"
	]
}
